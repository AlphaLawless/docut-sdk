{
  "name": "docut-sdk",
  "description": "Docut SDK",
  "module": "src/index.ts",
  "version": "1.0.0",
  "type": "module",
  "private": false,
  "scripts": {
    "check-ts": "tsc --noEmit",
    "dev": "bun src/index.ts",
    "build": "rm -rf dist && tsc --build",
    "lint:fix": "biome check --write ./src/**/*",
    "lint": "biome check ./src"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:KaikSelhorst/docut-sdk.git"
  },
  "keywords": [
    "docut",
    "sdk",
    "integration",
    "api",
    "typescript"
  ],
  "files": [
    "dist",
    "LICENSE"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": {
    "email": "oficial.kaikselhorst@gmail.com",
    "name": "Kaik Selhorst",
    "url": "https://github.com/KaikSelhorst"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@types/bun": "latest",
    "tsc-alias": "1.8.16"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  }
}
